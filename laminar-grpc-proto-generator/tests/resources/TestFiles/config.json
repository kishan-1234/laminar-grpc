{
  "server": {

      "port": 6000,

      "api_hook": "farm.nurture.example.service.BackendControllerImpl",

      "startup_hook": "farm.nurture.example.service.StartupHookImpl"

  },

  "prometheus" : {

      "enabled": false,

      "port": 6005

  },

  "temporal_worker": {

      "enabled": false,

      "namespace": "default",

